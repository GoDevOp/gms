[% META title = 'Search Results' %]

<table>
<tr>
  <th>Group Name</th>
  <th>Group Type</th>
  <th>Group Status</th>
</tr>

[% FOREACH group = results -%]
<tr>
  <td><a href='[% c.uri_for ("/staff/${group.id}/view") %]'>[% group.group_name %]</a></td>
  <td>[% group.group_type %]</td>
  <td>[% group.status %]</td>
</tr>
[% END -%]
</table>

<p>
  <a href='[% c.uri_for ("/staff/search_groups") %]'>Back to search</a>
</p>
